    "list-users": "npx tsx scripts/list-users.ts",
    "db:migrate": "supabase db reset",
    "db:generate-types": "supabase gen types typescript --local > lib/types/supabase.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.4",
    "@google/generative-ai": "^0.11.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsx": "^4.15.7",
    "typescript": "^5"
  },
  "keywords": ["backend", "supabase", "database", "scripts"],
