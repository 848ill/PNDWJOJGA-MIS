{
  "name": "pndwjogja-mis-backend",
  "version": "1.0.0",
  "description": "Backend utilities and database scripts for PNDW JOGJA MIS",
  "main": "index.js",
  "scripts": {
    "check-admin": "npx tsx scripts/check-admin.ts",
    "check-login": "npx tsx scripts/check-login.ts", 
    "clean-dummy": "npx tsx scripts/clean-dummy-data.ts",
    "debug-users": "npx tsx scripts/debug-users.ts",
